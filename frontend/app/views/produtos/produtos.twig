{% extends 'templates/main.twig' %}
{% block title%} Produtos - Intranet Quinta Valentina {% endblock %}

{% block content %}

    {% if url.parametros.var3 == 'update' and url.parametros.var4 is empty %}
        {% set urlRedirect = '/' ~ url.parametros.var1 ~ '/' ~ url.parametros.var2 ~ '/list' %}
        <script>window.location = '{{ urlRedirect }}';</script>
    {% else %}
        {% include('/' ~ url.parametros.var1 ~ '/' ~ url.parametros.var2 ~ '/' ~ url.parametros.var3 ~ '.twig') %}
    {% endif %}

{% endblock %}
